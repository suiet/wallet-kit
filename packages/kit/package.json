    "@types/lossless-json": "1.0.1",
    "@types/node": "20.4.2",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@vanilla-extract/css": "1.9.0",
    "@vanilla-extract/vite-plugin": "3.4.0",
    "@vitejs/plugin-react": "2.1.0",
    "jest": "28.1.3",
    "jest-simple-dot-reporter": "1.0.5",
    "postcss": "8.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
